<!DOCTYPE html>
<html>
  <head>
    <title>Dummy</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
  <% if flash[:danger] %>
    <%= sanitize(flash[:danger]) %>
  <% end %>
  <% if flash[:info] %>
    <%= sanitize(flash[:info]) %>
  <% end %>
  <% if flash[:success] %>
    <%= sanitize(flash[:success]) %>
  <% end %>
  <% if flash[:warning] %>
    <%= sanitize(flash[:warning]) %>
  <% end %>
    <%= yield %>
  </body>
</html>
